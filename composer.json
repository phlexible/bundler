{
    "name": "phlexible/bundler",
    "description": "phlexible bundler component",
    "license": "MIT",
    "type": "library",
    "keywords": ["phlexible"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Component\\Bundler\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.4",
        "phpunit/phpunit": "^5.7",
        "puli/discovery": "^1.0",
        "puli/repository": "^1.0",
        "symfony/config": "^2.8",
        "symfony/filesystem": "^2.8",
        "symfony/translation": "^2.8",
        "doctrine/collections": "^1.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
